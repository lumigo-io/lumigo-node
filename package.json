{
  "name": "lumigo-tracer-custom",
  "version": "1.0.0",
  "description": "Custom Lumigo tracer with enhanced data minimization and conditional tracing capabilities",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --build --force",
    "build:babel": "rm -rf dist && babel src --out-dir dist --extensions .ts,.js --source-maps",
    "build:commonjs": "npm run build && babel dist --out-dir dist --extensions .js --source-maps",
    "check-types": "tsc --noEmit",
    "dev": "babel src -d lib --watch",
    "test": "jest"
  },
  "dependencies": {
    "@lumigo/tracer": "^1.0.0",
    "aws-sdk": "^2.1000.0",
    "debug": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "jest": "^29.0.0"
  },
  "keywords": [
    "lumigo",
    "tracer",
    "lambda",
    "aws",
    "observability",
    "data-minimization"
  ],
  "author": "Your Name",
  "license": "MIT"
}
