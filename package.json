{
  "name": "@lumigo/tracer",
  "version": "1.60.1",
  "description": "Lumigo Tracer for Node.js v8.x / v10.x / v12.x Runtimes",
  "main": "dist/index.js",
  "repository": "git@github.com:lumigo-io/lumigo-node.git",
  "private": false,
  "author": "Lumigo LTD (https://lumigo.io)",
  "contributors": [
    "Sagi Kedmi (https://sagi.io)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -rf dist && tsc --build --force",
    "jest": "jest",
    "semantic-release": "semantic-release",
    "test": "npm run build && npm run jest",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run prettier:fix && npm run lint && npm run test",
    "prettier:ci": "prettier --list-different \"src/**/*.[jt]s\" --parser typescript",
    "prettier:fix": "prettier --write \"./src/**/*.[jt]s\" --parser typescript"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "0.21.1",
    "shimmer": "1.2.1",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/github": "^7.2.3",
    "@types/aws-lambda": "^8.10.77",
    "@types/jest": "^26.0.23",
    "@types/utf8": "^3.0.0",
    "aws-sdk": "^2.927.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^8.1.0",
    "husky": "^2.4.1",
    "jest": "^27.0.4",
    "jest-each": "^26.6.2",
    "jest-junit": "^6.4.0",
    "mockdate": "^2.0.5",
    "mockingoose": "^2.15.2",
    "mongo-mock": "^4.0.0",
    "mongoose": "5.12.2",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.3",
    "typescript": "^4.3.2"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
