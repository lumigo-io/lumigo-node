{
  "name": "@lumigo/tracer",
  "version": "1.43.0",
  "description": "Lumigo Tracer for Node.js v8.x / v10.x Runtimes",
  "main": "dist/lumigo.js",
  "repository": "git@github.com:lumigo-io/lumigo-node.git",
  "private": false,
  "author": "Lumigo LTD (https://lumigo.io)",
  "contributors": [
    "Sagi Kedmi (https://sagi.io)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build:babel": "babel --ignore '**/*.test.js' --ignore testdata src -d lib --verbose",
    "build:webpack": "webpack --display-modules ./lib -o dist/lumigo.js",
    "build": "npm run build:babel && npm run build:webpack",
    "jest": "jest",
    "semantic-release": "semantic-release",
    "test": "npm run build && npm run jest",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run prettier:fix && npm run lint && npm run test",
    "prettier:ci": "prettier --list-different \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"./src/**/*.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.4.4",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/jest": "^26.0.8",
    "agentkeepalive": "^4.1.3",
    "aws-sdk": "^2.465.0",
    "axios": "0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "json.sortify": "^2.2.2",
    "lambda-local": "^1.6.2",
    "mockdate": "^2.0.2",
    "mongo-mock": "^4.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "semantic-release": "^15.13.19",
    "shimmer": "^1.2.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
